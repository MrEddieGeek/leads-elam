{
  "name": "leads-generator",
  "version": "1.0.0",
  "description": "Lead generation tool for logistics sales team",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "db:migrate": "node server/db/migrate.js"
  },
  "keywords": ["leads", "scraping", "logistics", "sales"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "puppeteer-core": "^21.6.1",
    "exceljs": "^4.4.0",
    "date-fns": "^3.0.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  }
}

{
  "name": "leads-generator",
  "version": "1.0.0",
  "description": "Lead generation tool for logistics sales team",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "cd client && npm run dev",
    "db:migrate": "node server/db/migrate.js"
  },
  "keywords": [
    "leads",
    "scraping",
    "logistics",
    "sales"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^7.0.0",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "date-fns": "^3.0.6",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "prisma": "^7.0.0",
    "puppeteer-core": "^24.31.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "vite": "^7.2.4"
  }
}
